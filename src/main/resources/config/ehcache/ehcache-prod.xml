<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap unit="entries">1000</heap>
    </cache-template>

    <cache alias="pe.gob.congreso.didp.agenda.domain.User" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Authority" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.User.authorities" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.PersistentToken" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Agenda" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Agenda.temaAgendas" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Comision" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Comision.temaAgenda" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Seccion" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.Seccion.seccions" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.TemaAgenda" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.TemaAgenda.seccions" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.TemaAgenda.comisions" uses-template="simple"/>

    <cache alias="pe.gob.congreso.didp.agenda.domain.TextoSustitutorio" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
